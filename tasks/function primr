#include <stdio.h>
#include <math.h>

// Function to check prime
int isPrime(int n) {
    if (n <= 1) return 0;  // numbers <= 1 are not prime
    for (int i = 2; i <= sqrt(n); i++) {
        if (n % i == 0) return 0;  // divisible â†’ not prime
    }
    return 1;  // prime
}

int main() {
    int start, end;

    printf("Enter start of interval: ");
    scanf("%d", &start);

    printf("Enter end of interval: ");
    scanf("%d", &end);

    printf("Prime numbers between %d and %d are:\n", start, end);

    for (int num = start; num <= end; num++) {
        if (isPrime(num)) {
            printf("%d ", num);
        }
    }

    return 0;
}
