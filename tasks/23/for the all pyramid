#include <stdio.h>

int main() {
    int choice, n, i, j;

    printf("Enter the number of rows: ");
    scanf("%d", &n);

    printf("\nChoose Pyramid Type:\n");
    printf("1. Right-Angled Star Pyramid\n");
    printf("2. Inverted Right-Angled Star Pyramid\n");
    printf("3. Full Star Pyramid (Centered)\n");
    printf("4. Inverted Full Star Pyramid\n");
    printf("5. Number Pyramid\n");
    printf("Enter your choice (1-5): ");
    scanf("%d", &choice);

    printf("\n");

    switch(choice) {
        case 1: // Right-Angled Star Pyramid
            for(i = 1; i <= n; i++) {
                for(j = 1; j <= i; j++)
                    printf("* ");
                printf("\n");
            }
            break;

        case 2: // Inverted Right-Angled Star Pyra
