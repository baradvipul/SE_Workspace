#include <stdio.h>
#include <string.h>  // Required for strcpy()

int main() {
    char source[] = "Hello, World!";
    char destination[50];

    // Copy source string to destination
    strcpy(destination, source);

    printf("Source: %s\n", source);
    printf("Destination: %s\n", destination);

    return 0;
}
